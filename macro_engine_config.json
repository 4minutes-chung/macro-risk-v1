{
  "inputs": {
    "model_panel_csv": "data/macro_panel_quarterly_model.csv"
  },
  "horizons": {
    "short_quarters": 8,
    "medium_quarters": 20,
    "long_start_quarter": 21,
    "total_quarters": 80
  },
  "model": {
    "variables": [
      "unemployment_rate",
      "labor_force_participation",
      "payroll_growth_yoy",
      "wage_growth_yoy",
      "headline_cpi_yoy",
